###
###  vlc 3.0.8
###

###
### lines beginning with a '#' character are comments
###

[visual] # Visualizer filter

# Effects list (字符串)
#effect-list=spectrum

# Video width (整数)
#effect-width=800

# Video height (整数)
#effect-height=500

# FFT window (字符串)
#effect-fft-window=flat

# Kaiser window parameter (浮点)
#effect-kaiser-param=3.000000

# Show 80 bands instead of 20 (布尔)
#visual-80-bands=1

# Draw peaks in the analyzer (布尔)
#visual-peaks=1

# Enable original graphic spectrum (布尔)
#spect-show-original=0

# Draw the base of the bands (布尔)
#spect-show-base=1

# Base pixel radius (整数)
#spect-radius=42

# Spectral sections (整数)
#spect-sections=3

# V-plane color (整数)
#spect-color=80

# Draw bands in the spectrometer (布尔)
#spect-show-bands=1

# Show 80 bands instead of 20 (布尔)
#spect-80-bands=1

# Number of blank pixels between bands. (整数)
#spect-separ=1

# Amplification (整数)
#spect-amp=8

# Draw peaks in the analyzer (布尔)
#spect-show-peaks=1

# Peak extra width (整数)
#spect-peak-width=61

# Peak height (整数)
#spect-peak-height=1

